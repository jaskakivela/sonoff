{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "labels": {
            "app": "sandbox"
        },
        "name": "sonoff-server",
        "namespace": "sonoff"
    },
    "spec": {
        "ports": [
            {
                "name": "http",
                "port": 8080,
                "protocol": "TCP",
                "targetPort": 8080
            },
            {
                "name": "https",
                "port": 8443,
                "protocol": "TCP",
                "targetPort": 8443
            }
        ],
        "selector": {
            "deploymentconfig": "sandbox"
        },
        "sessionAffinity": "None",
        "type": "ClusterIP"
    }
}
